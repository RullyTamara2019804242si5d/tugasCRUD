    private void btnsimpanActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btnsimpanActionPerformed
        try{
           Connection koneksi;
            koneksi = DriverManager.getConnection("jdbc:mysql://localhost:3306/crud","root","");
           koneksi.createStatement().executeUpdate("insert into Laporan_kas values "+"('"+txno.getText()+"','"+txtgl.getText()+"','"+txsmbr.getText()+"','"+txket.getText()+"','"+txnominal.getText()+"')");
           tampilkan();      
      } catch (SQLException s) {
            Logger.getLogger(formdata.class.getName()).log(Level.SEVERE, null, s);
        }
    }        // TODO add your handling code here:

    private void tampilkan() {
        throw new UnsupportedOperationException("Not supported yet."); //To change body of generated methods, choose Tools | Templates.
    }
    }//GEN-LAST:event_btnsimpanActionPerformed

    private void btnhapusActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btnhapusActionPerformed
        try{
           Connection koneksi =  DriverManager.getConnection("jdbc:mysql://localhost:3306/crud","root","");
           koneksi.createStatement().executeUpdate("delete from Laporan_kas where id='"+txno.getText()+"'");
           tampilkan();      
      } catch (SQLException ex) {
            Logger.getLogger(formdata.class.getName()).log(Level.SEVERE, null, ex);
        }        // TODO add your handling code here:
    }//GEN-LAST:event_btnhapusActionPerformed

