    private void antonumber() throws SQLException{
        try{
            Connection c = Koneksi.getKoneksi();
            Statement  s = (Statement) c.createStatement();
            String sql = "SELECT * FROM Laporan_kas ORDER BY id DESC";
            ResultSet r = s.executeQuery(sql);
            if (r.next()){
                String NoTransaksi;
                NoTransaksi = r.getString("id").substring(2);
                String NT = "" + (Interger.parseInt(NoTransaksi) +1);
                String No1 = "";
                
                switch (NT.length()) {
                    case 1:
                        No1 = "00";
                        break;
                    case 2:
                        No1 = "0";
                        break;
                    case 3:
                        No1 = "";
                        break;
                    default:
                        break;
                }
                
                txno.setText("Nt" + No1 + NT);
                
            }else{
                txno.setText("NT001");
            }
            r.close();
            s.close();    
        }catch (SQLException e){
            System.out.println("autonumber error");
        }
    }  
    
    public void clear(){
        txtgl.setText("");
        txsmbr.setText("");
        txket.setText("");
        txnominal.setText("");
    }
    
    public void loadData() throws SQLException{
        model.getDataVector().removeAllElements();
        model.fireTableDataChanged();
        
           try{
            Connection c = Koneksi.getKoneksi();
            Statement s = (Statement) c.createStatement();
            
            String sql = "SELECT * FROM Laporan_kas";
            ResultSet r = s.executeQuery(sql);
            
            while(r.next())}
                Object[] o = new Object[4];
                o [0] = r.getString("No")
        }catch(SQLException e)static {
    }
    
